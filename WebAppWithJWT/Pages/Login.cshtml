@page
@model WebAppWithJWT.Pages.LoginModel
@{
    ViewData["Title"] = "Login";
}

<h2>Login</h2>

<form method="post">
    <div>
        <label>Username</label>
        <input asp-for="LoginData.Username" />
    </div>
    <div>
        <label>Password</label>
        <input asp-for="LoginData.Password" type="password" />
    </div>
    <button type="submit">Login</button>
</form>


